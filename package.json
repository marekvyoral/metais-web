{
    "name": "metais",
    "version": "0.1.11",
    "private": true,
    "workspaces": [
        "app/*",
        "packages/*"
    ],
    "dependencies": {
        "@casl/ability": "^6.5.0",
        "@casl/react": "^3.1.0",
        "@hookform/error-message": "^2.0.1",
        "@hookform/resolvers": "^3.1.1",
        "@id-sk/frontend": "^2.10.0",
        "@isdd/idsk-ui-kit": "*",
        "@isdd/metais-common": "*",
        "@tanstack/react-query": "^4.29.7",
        "@tanstack/react-table": "^8.9.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/d3": "^7.4.0",
        "@types/jest": "^27.0.1",
        "@types/luxon": "^3.3.1",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/uuid": "^9.0.2",
        "@uppy/core": "^3.2.0",
        "@uppy/dashboard": "^3.4.0",
        "@uppy/drag-drop": "^3.0.2",
        "@uppy/file-input": "^3.0.2",
        "@uppy/locales": "^3.2.1",
        "@uppy/progress-bar": "^3.0.2",
        "@uppy/react": "^3.1.2",
        "@uppy/status-bar": "^3.1.2",
        "@uppy/utils": "^5.3.0",
        "@uppy/xhr-upload": "^3.2.0",
        "@xstate/react": "^3.2.2",
        "canvg": "^3.0.10",
        "classnames": "^2.3.2",
        "d3": "^7.8.5",
        "diff-match-patch": "^1.0.5",
        "i18next": "^22.4.15",
        "i18next-http-backend": "^2.2.0",
        "jspdf": "^2.5.1",
        "jwt-decode": "^3.1.2",
        "luxon": "^3.3.0",
        "patch-package": "^7.0.0",
        "react": "^18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-i18next": "^12.2.2",
        "react-modal": "^3.16.1",
        "react-quill": "^2.0.0",
        "react-router-dom": "^6.11.1",
        "react-select": "^5.7.3",
        "react-select-async-paginate": "^0.7.2",
        "react-sticky-box": "^2.0.4",
        "react-tooltip": "^5.14.0",
        "reactjs-popup": "^2.0.5",
        "sanitize-html": "^2.11.0",
        "sift": "^17.0.1",
        "typescript": "^4.4.2",
        "uuid": "^9.0.0",
        "web-vitals": "^2.1.0",
        "xstate": "^4.38.2",
        "yup": "^1.2.0"
    },
    "scripts": {
        "start": "yarn start:portal & yarn start:admin & yarn start:login && fg",
        "start:portal": "vite app/metais-portal",
        "start:admin": "vite app/metais-admin",
        "start:login": "vite app/metais-login",
        "build": "yarn build:portal && yarn build:admin && yarn build:login",
        "build:development": "yarn build:portal-development && yarn build:admin-development && yarn build:login-development",
        "build:mirri-test": "yarn build:portal-mirri-test && yarn build:admin-mirri-test && yarn build:login-mirri-test",
        "build:portal": "vite build app/metais-portal",
        "build:portal-development": "vite build app/metais-portal --mode development",
        "build:portal-mirri-test": "vite build app/metais-portal --mode mirri-test",
        "build:script:transform": "tsc packages/metais-common/src/api/hooks/transform/attributesTypesTransformer.ts --outDir packages/metais-common/scripts/",
        "build:admin": "vite build app/metais-admin",
        "build:admin-development": "vite build app/metais-admin --mode development",
        "build:admin-mirri-test": "vite build app/metais-admin --mode mirri-test",
        "build:login": "vite build app/metais-login",
        "build:login-development": "vite build app/metais-login --mode development",
        "build:login-mirri-test": "vite build app/metais-login --mode mirri-test",
        "preview": "yarn preview:portal & yarn preview:admin && fg",
        "preview:portal": "vite preview app/metais-portal",
        "preview:admin": "vite preview app/metais-admin",
        "preview:login": "vite preview app/metais-login",
        "typescript-check": "yarn typescript-check:portal && yarn typescript-check:admin && yarn typescript-check:idsk-ui-kit && yarn typescript-check:common && yarn typescript-check:login",
        "typescript-check:portal": "tsc --noEmit -p app/metais-portal/tsconfig.json",
        "typescript-check:admin": "tsc --noEmit -p app/metais-admin/tsconfig.json",
        "typescript-check:login": "tsc --noEmit -p app/metais-login/tsconfig.json",
        "typescript-check:idsk-ui-kit": "tsc --noEmit -p packages/idsk-ui-kit/tsconfig.json",
        "typescript-check:common": "tsc --noEmit -p packages/metais-common/tsconfig.json",
        "lint": "yarn lint:portal && yarn lint:admin && yarn lint:idsk-ui-kit && yarn lint:common && yarn lint:login",
        "lint:portal": "cd app/metais-portal && eslint . --max-warnings 0",
        "lint:admin": "cd app/metais-admin && eslint . --max-warnings 0",
        "lint:login": "cd app/metais-login && eslint . --max-warnings 0",
        "lint:idsk-ui-kit": "cd packages/idsk-ui-kit && eslint . --max-warnings 0",
        "lint:common": "cd packages/metais-common && eslint . --max-warnings 0",
        "lint-fix": "yarn lint-fix:portal && yarn lint-fix:admin && yarn lint-fix:idsk-ui-kit && yarn lint-fix:common && yarn lint-fix:login",
        "lint-fix:portal": "cd app/metais-portal && eslint . --fix",
        "lint-fix:admin": "cd app/metais-admin && eslint . --fix",
        "lint-fix:login": "cd app/metais-login && eslint . --fix",
        "lint-fix:idsk-ui-kit": "cd packages/idsk-ui-kit && eslint . --fix",
        "lint-fix:common": "cd packages/metais-common && eslint . --fix",
        "test": "vitest",
        "prepare": "yarn prepare:portal && yarn prepare:admin && yarn prepare:login",
        "prepare:portal": "recursive-copy -w node_modules/@id-sk/frontend/govuk/assets app/metais-portal/public/assets && recursive-copy -w node_modules/@id-sk/frontend/idsk/assets app/metais-portal/public/assets",
        "prepare:admin": "recursive-copy -w node_modules/@id-sk/frontend/govuk/assets app/metais-admin/public/assets && recursive-copy -w node_modules/@id-sk/frontend/idsk/assets app/metais-admin/public/assets",
        "prepare:login": "recursive-copy -w node_modules/@id-sk/frontend/govuk/assets app/metais-login/public/assets && recursive-copy -w node_modules/@id-sk/frontend/idsk/assets app/metais-login/public/assets",
        "storybook:idsk-ui-kit": "cd packages/idsk-ui-kit && storybook dev -p 6006 --disable-telemetry",
        "storybook:common": "cd packages/metais-common && storybook dev -p 6006 --disable-telemetry",
        "storybook:portal": "cd app/metais-portal && storybook dev -p 6006 --disable-telemetry",
        "build-storybook": "cd app/metais-portal && storybook build",
        "postinstall": "patch-package",
        "generate-api-development": "yarn build:script:transform && cross-env DOTENV_CONFIG_PATH=./.env.development orval --config ./orval.config.ts",
        "generate-api-production": "yarn build:script:transform && cross-env DOTENV_CONFIG_PATH=./.env.production orval --config ./orval.config.ts"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=16.0",
        "yarn": "=1"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.0.12",
        "@storybook/addon-interactions": "^7.0.12",
        "@storybook/addon-links": "^7.0.12",
        "@storybook/blocks": "^7.0.12",
        "@storybook/react": "^7.0.12",
        "@storybook/react-vite": "^7.0.12",
        "@storybook/testing-library": "^0.0.14-next.2",
        "@types/diff-match-patch": "^1.0.33",
        "@types/react-modal": "^3.16.0",
        "@types/sanitize-html": "^2.9.0",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.5",
        "@vitejs/plugin-react": "^4.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.12",
        "orval": "^6.16.0",
        "prettier": "^2.8.8",
        "prop-types": "^15.8.1",
        "recursive-copy-cli": "^1.0.20",
        "sass": "^1.62.1",
        "storybook": "^7.0.12",
        "vite": "^4.3.5",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.32.0"
    }
}
